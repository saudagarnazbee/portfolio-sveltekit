{
  "name": "portfolio-sveltekit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development vite dev",
    "build": "npm run clean && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "deploy": "npm run build && gh-pages -d build -t",
    "clean": "trash build"
  },
  "devDependencies": {
    "@poppanator/sveltekit-svg": "^2.0.2",
    "@responsive-ui/table": "^1.1.0-alpha.8",
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/adapter-static": "^1.0.0",
    "@sveltejs/kit": "^1.0.0",
    "@tailwindcss/typography": "^0.5.7",
    "autoprefixer": "^10.4.8",
    "dayjs": "^1.11.5",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "gh-pages": "^4.0.0",
    "lottie-web": "^5.9.6",
    "postcss": "^8.4.16",
    "prettier": "^2.8.0",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "svelte-confetti": "^1.2.0",
    "svelte-preprocess": "^4.10.7",
    "svelte-typewriter": "^3.1.0",
    "tailwindcss": "^3.1.8",
    "trash-cli": "^5.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-imagetools": "^4.0.5",
    "vite-plugin-markdown": "^2.2.0-0"
  },
  "type": "module"
}
